class Solution {
    static String isSumOfTwo(int N){
        boolean[] arr = new boolean[N+1];
        arr[0] = true;
        arr[1] = true;
        for(int i=2;i<arr.length;i++){
            if(arr[i]==false){
                for(int j=2;i*j<=N;j++){
                    arr[i*j] = true;
                }
            }
        }
        
        for(int i=2;i<=N/2;i++){
            if(!arr[i] && !arr[N-i]){
                return "Yes";
            }
        }
        return "No";
    }
}
