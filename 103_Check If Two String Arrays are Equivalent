/*
 * @lc app=leetcode id=1662 lang=java
 *
 * [1662] Check If Two String Arrays are Equivalent
 */

// @lc code=start
class Solution {
    public boolean arrayStringsAreEqual(String[] word1, String[] word2) {
        String str1 = "";
        for (String i : word1) {
            str1 = str1 + i;
        }
        String str2 = "";
        for (String i : word2) {
            str2 = str2 + i;
        }
        return str1.equals(str2);
    }
}
// @lc code=end
